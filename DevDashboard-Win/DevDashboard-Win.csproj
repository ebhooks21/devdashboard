<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <RootNamespace>DevDashboard_Win</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>devdashboardlogo.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="devdashboardlogo.ico" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\DevDashboard.controllers\DevDashboard.controllers.csproj" />
    <ProjectReference Include="..\DevDashboard.models\DevDashboard.models.csproj" />
  </ItemGroup>

</Project>